{
  "expo": {
    "name": "picture Api Unsplash",
    "slug": "picture-api",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon-flag.png",
    "splash": {
      "image": "./assets/splash-governor.png",
      "resizeMode": "contain",
      "backgroundColor": "#C9CFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.t-1000-t.app-imagefined-rn",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.t_1000_t.app_imagefined_rn",
      "versionCode": 1,
      "permissions": ["WRITE_EXTERNAL_STORAGE"]
    }
  }
}
